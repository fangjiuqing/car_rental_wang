        <section class="tip-order-status">
            <p>
                <span class="icon-alert"></span>
                <span class="status-text">您有一个订单进行中</span>
                <span class="icon-arc-right"></span>
            </p>
        </section>
        <section class="scroll-view">
            <div class="swiper-container">
                <div class="swiper-wrapper">
                    <!-- <a class="swiper-slide">
                        Slide 1
                        <img src="" alt="">
                    </a> -->
                </div>
                <div class="swiper-pagination"></div>
            </div>
        </section>
        <section class="select-car">
            <div class="tip-select">
                <p>温馨提示：选择上门送还车需要提前4小时下单</p>
            </div>
            <!-- type: [delivery(上门) | arrive(到店)] -->
            <div class="select-store" data-type="">
                <div class="store-wrap get-store" data-serviceType="arrive">
                    <div class="select-city get-city" data-city-type="get">北京</div>
                    <div class="select-address get-address" >
                        <input type="text" placeholder="请选择地址" data-address-type="get" readonly />
                        <!-- active -->
                        <span class="tip-extra">
                            该门店仅支持自助取车
                        </span>
                    </div>
                    <!-- type-delivery | type-arrive -->
                    <div class="select-type get-type type-arrive" data-select-type="get">
                        <span class="delivery">上门</span>
                        <span class="arrive">到店</span>
                    </div>
                </div>
                <div class="store-wrap return-store" data-serviceType="arrive">
                    <div class="select-city return-city" data-city-type="return">北京</div>
                    <div class="select-address return-address">
                        <input type="text" placeholder="请选择地址" data-address-type="return" readonly />
                        <span class="tip-extra">
                            该门店仅支持自助还车
                        </span>
                    </div>
                    <div class="select-type return-type type-arrive" data-select-type="return">
                        <span class="delivery">上门</span>
                        <span class="arrive">到店</span>
                    </div>
                </div>
            </div>
            <div class="select-date">
                <div class="date-wrap start-date" data-date-type="start">
                    <!-- 05月05日 -->
                    <span class="month-day">11月09日</span>
                    <!-- 周五 12：30 -->
                    <span class="week-time">周六 10:00</span>
                </div>
                <div class="duration">
                    <!-- 2天 -->
                    <span class="dur-days"></span>
                    <span class="decoration"></span>
                </div>
                <div class="date-wrap end-date" data-date-type="end">
                    <span class="month-day"></span>
                    <span class="week-time"></span>
                </div>
            </div>
            <div class="select-btn">
                <button>立即选车</button>
            </div>
        </section>



        <footer>
            <p class="tel">
